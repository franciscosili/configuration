# -*- mode: sh -*-
# bash aliases

# ls
alias ls='ls'
alias sl='ls'
alias ll='ls -lh'
alias la='ls -la'
alias lt='ls | sort -k 2 -k 1M -k2'

# emacs
alias emacs='emacs -nw'
alias emw='emacs'
alias ewm='emacs'
alias em='emacs'
alias e='emacs'


# root
alias root='root -l'
alias rootq='root -q'

# clear
alias cl="clear"
